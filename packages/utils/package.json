{
  "name": "@shared/utils",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "build": "tsc -b",
    "pack": "npm pack --pack-destination ../",
    "dist": "npm run build && npm run pack"
  },
  "dependencies": {
    "@shared/types": "file:../types"
  },
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./ts": {
      "import": "./src/index.ts",
      "types": "./dist/src/index.d.ts"
    }
  }
}